BEGIN:VCALENDAR
VERSION:2.0
X-WR-CALNAME:{{ site.longname }}
X-WR-TIMEZONE:{{ site.timezone }}
X-PUBLISHED-TTL:PT2H
PRODID:{{ site.url }}
METHOD:PUBLISH
{{#each schedules}}
{{#each meetings}}
BEGIN:VEVENT
UID:{{ ../site.url }}/{{ date day 'YYYYMMDD' }}
LOCATION:{{ ../site.location }}
SUMMARY:{{ title }}
DESCRIPTION:{{ who }}
CLASS:PUBLIC
DTSTART;TZID={{ ../site.timezone }}:{{ date day 'YYYYMMDD' }}T{{ time ../site.time 'hhmmss' }}
DTEND;TZID={{ ../site.timezone }}:{{ date day 'YYYYMMDD' }}T{{ time ../site.endtime 'hhmmss' }}
LAST-MODIFIED:{{ now "YYYYMMDDTHHMMSS" }}
SEQUENCE:1
END:VEVENT
{{/each}}
{{/each}}
END:VCALENDAR
